<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunpay.permission.dao.impl.AttachmentDaoImpl">

	<!-- 根据附件id查询附件信息 -->
	<select id="selectByPrimaryKey" parameterType="java.lang.Long" resultType="com.yunpay.permission.entity.Attachment">
		select * from t_sys_attach where id = #{attachId}
	</select>
</mapper>